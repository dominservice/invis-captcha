{
  "name": "dominservice/invis-captcha",
  "description": "Invisible, score-based CAPTCHA for Laravel.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "dominservice",
      "email": "biuro@dso.biz.pl",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0",
    "firebase/php-jwt": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0",
    "mockery/mockery": "^1.5"
  },
  "autoload": {
    "psr-4": { "Dominservice\\Invisible\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Dominservice\\Invisible\\Tests\\": "tests/" }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": { "providers": ["Dominservice\\Invisible\\InvisibleServiceProvider"] }
  }
}
